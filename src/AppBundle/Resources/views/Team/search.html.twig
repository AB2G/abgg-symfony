{% extends '::base.html.twig' %}

{% block body -%}
<h1>Liste des équipes</h1>

<table  class="table table-hover">
  <thead>
    <tr>
      <th>Equipe</th>
      <th>Capitaine d'équipe</th>
    </tr>
  </thead>
  <tbody>
    {% for entity in entities %}
    <tr>
      <td>{{ entity.name }}</td>
      <td>{{ entity.captain }}</td>
    </tr>
    {% endfor %}
  </tbody>
</table>
{% endblock %}
